{
  "IconAndTextCta": {
    "type": "IconAndTextCta",
    "category": "Action",
    "description": "A call to action button that can have an icon, text or both",
    "parameterTypes": {
      "icon": {
        "dataType": "URL",
        "required": false,
        "description": "URL containing the icon to display on the left",
        "examples": [
          "https://raw.githubusercontent.com/martingalecorp/Pact-Marketing-External/master/Pact_Logo_336x116.png"
        ]
      },
      "text": {
        "required": false,
        "description": "The text that will appear in the header",
        "examples": [
          "What is your license number?",
          "What is your gender?"
        ]
      }
    },
    "defaultStyle": {
      "properties": {
        "fontFamily": "Sofia Pro",
        "fontWeight": "normal",
        "fontSize": "17px",
        "lineHeight": "0px",
        "color": "#484EAA"
      }
    }
  },
  "PrimaryCtaSlider": {
    "type": "PrimaryCtaSlider",
    "category": "Action",
    "description": "The third most important heading",
    "parameterTypes": {
      "title": {
        "dataType": "DynamicString",
        "required": true,
        "description": "Main call to action text",
        "examples": [
          "Continue",
          "Confirm",
          "Pay Now",
          "Sign Up"
        ]
      }
    },
    "defaultStyle": {
      "properties": {
        "fontFamily": "Sofia Pro",
        "fontWeight": "normal",
        "fontSize": "17px",
        "lineHeight": "0px",
        "color": "#484EAA"
      }
    }
  },
  "SummaryEditCell": {
    "type": "SummaryEditCell",
    "category": "Action",
    "description": "The third most important heading",
    "parameterTypes": {
      "upperText": {
        "dataType": "DynamicString",
        "required": true,
        "description": "The text that will appear in the top half of the cell",
        "examples": [
          "July 1991"
        ]
      },
      "lowerText": {
        "dataType": "DynamicString",
        "required": true,
        "description": "The text that will appear in the bottom half of the cell",
        "examples": [
          "Driving under the influence"
        ]
      }
    },
    "defaultStyle": {
      "properties": {
        "fontFamily": "Sofia Pro",
        "fontWeight": "normal",
        "fontSize": "17px",
        "lineHeight": "100%",
        "color": "#94949E"
      }
    }
  },
  "SingleSelect": {
    "type": "SingleSelect",
    "category": "DataField",
    "description": "The third most important heading",
    "parameterTypes": {
      "title": {
        "dataType": "DynamicString",
        "required": true,
        "description": "Short description of data you want the user to input",
        "examples": [
          "Occupation"
        ]
      },
      "selectables": {
        "dataType": "Array",
        "required": true,
        "description": "The set of available options the user can select from",
        "examples": [
          [
            {
              "label": "Hit Pedestrian",
              "value": "AAF"
            },
            {
              "label": "Swerved / Lost Control",
              "value": "AAF"
            },
            {
              "label": "Acts of Nature / Weather-related",
              "value": "COMP"
            }
          ]
        ]
      }
    }
  },
  "H1": {
    "type": "H1",
    "category": "TextBox",
    "description": "The most important heading",
    "parameterTypes": {
      "text": {
        "dataType": "DynamicString",
        "required": true,
        "description": "The text that will appear in the header",
        "examples": [
          "What is your license number?",
          "What is your gender?"
        ]
      }
    },
    "defaultStyle": {
      "properties": {
        "fontFamily": "Sofia Pro",
        "fontWeight": "normal",
        "fontSize": "28px",
        "lineHeight": "100%",
        "color": "#2C2D36"
      }
    }
  },
  "H2": {
    "type": "H2",
    "category": "TextBox",
    "description": "The second most important heading",
    "parameterTypes": {
      "text": {
        "dataType": "DynamicString",
        "required": true,
        "description": "The text that will appear in the header",
        "examples": [
          "Please make sure the entered license number is correct."
        ]
      }
    },
    "defaultStyle": {
      "properties": {
        "fontFamily": "Sofia Pro",
        "fontWeight": "bold",
        "fontSize": "17px",
        "lineHeight": "100%",
        "color": "#2C2D36"
      }
    }
  },
  "H3": {
    "type": "H3",
    "category": "TextBox",
    "description": "The third most important heading",
    "parameterTypes": {
      "text": {
        "dataType": "DynamicString",
        "required": true,
        "description": "The text that will appear in the header",
        "examples": [
          "What is your license number?",
          "What is your gender?"
        ]
      }
    },
    "defaultStyle": {
      "properties": {
        "fontFamily": "Sofia Pro",
        "fontWeight": "normal",
        "fontSize": "17px",
        "lineHeight": "100%",
        "color": "#94949E"
      }
    }
  }
}
